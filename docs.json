{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "ManyPI Docs",
  "colors": {
    "primary": "#2c3748",
    "light": "#1b202c",
    "dark": "#4a5568"
  },
  "favicon": "/manypi-icon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/authentication",
              "features/website-to-api",
              "features/type-safety",
              "features/rate-limiting"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/scrape"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Changelog",
          "href": "https://manypi.com/changelog",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://manypi.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/manypi-logo.webp",
    "dark": "/logo/manypi-logo.webp"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@manypi.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Homepage",
      "href": "https://manypi.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/manypicom",
      "linkedin": "https://linkedin.com/company/manypi"
    }
  }
}
